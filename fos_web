            function s1() {
              let x, y;
              x = parseInt(prompt("Number 1: "));
              y = parseInt(prompt("Number 2: "));
              alert(x + y);
            }
            function s2() {
              let x, y;
              x = parseInt(prompt("Number 1: "));
              y = parseInt(prompt("Number 2: "));
              alert(x - y);
            }
            function s3() {
              let x, y;
              x = parseInt(prompt("Number 1: "));
              y = parseInt(prompt("Number 2: "));
              alert(x * y);
            }
            function s4() {
              let x, y;
              x = parseInt(prompt("Number 1: "));
              y = parseInt(prompt("Number 2: "));
              alert(x / y);
            }
            function s5() {
              let x, y;
              x = parseInt(prompt("Number 1: "));
              y = parseInt(prompt("Number 2: "));
              alert(x % y);
            }
            function s6() {
              let x, y;
              x = parseInt(prompt("Number 1: "));
              y = parseInt(prompt("Number 2: "));
              alert(x ** y);
            }
            </script>
            <h2>Calculator</h2>
            <button onclick="s1()"> Addition (+) </button>
            <button onclick="s2()"> Subtraction (-) </button>
            <button onclick="s3()"> Multiplication (x) </button>
            <button onclick="s4()"> Division (/) </button>
            <button onclick="s5()"> Modulo (%) </button>
            <button onclick="s6()"> Exponentation (^) </button>
